# Cloudinary Credentials
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret

# Google Gemini API
GOOGLE_API_KEY=your_gemini_api_key

# Logging & Monitoring
LOG_LEVEL=info
LOKI_HOST=https://your-loki-host
LOKI_BASIC_AUTH=user:pass  # Optional
LOKI_JOB_NAME=retroscope   # Optional
LOKI_COMPONENT=image-analysis  # Optional
LOKI_ENVIRONMENT=production    # Optional
NODE_ENV=production

# Optional Configuration
BATCH_SIZE=100  # Number of images to process per run 